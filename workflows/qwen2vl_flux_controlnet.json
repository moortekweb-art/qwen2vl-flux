{
  "1": {
    "inputs": {
      "image": "/path/to/your/image.png"
    },
    "class_type": "LoadImage",
    "_meta": {
      "title": "Load Input Image"
    }
  },
  "2": {
    "inputs": {
      "image": [
        "1",
        0
      ],
      "apply_attention": false
    },
    "class_type": "Qwen2VLImageEncoder",
    "_meta": {
      "title": "Encode Input with Qwen2VL"
    }
  },
  "3": {
    "inputs": {
      "image": [
        "1",
        0
      ],
      "control_type": "both",
      "canny_low_threshold": 50,
      "canny_high_threshold": 150
    },
    "class_type": "ControlNetProcessor",
    "_meta": {
      "title": "Extract Control (Depth + Line)"
    }
  },
  "4": {
    "inputs": {
      "control_image": [
        "3",
        0
      ],
      "blur_kernel": 5
    },
    "class_type": "ControlNetDenoiser",
    "_meta": {
      "title": "Denoise Control Map"
    }
  },
  "5": {
    "inputs": {
      "qwen_embedding": [
        "2",
        0
      ],
      "qwen_grid_thw": [
        "2",
        1
      ],
      "prompt": "A futuristic city with neon lights",
      "mode": "controlnet",
      "guidance_scale": 3.5,
      "num_inference_steps": 28,
      "aspect_ratio": "16:9",
      "batch_size": 1,
      "seed": 42,
      "input_image": [
        "1",
        0
      ],
      "enable_depth_control": true,
      "enable_line_control": true,
      "depth_strength": 0.3,
      "line_strength": 0.5,
      "denoise_strength": 0.7
    },
    "class_type": "FLUXGeneratorDualGPU",
    "_meta": {
      "title": "Generate with FLUX (ControlNet)"
    }
  },
  "6": {
    "inputs": {
      "images": [
        "5",
        0
      ],
      "filename_prefix": "qwen2vl_flux_controlnet"
    },
    "class_type": "SaveImage",
    "_meta": {
      "title": "Save Output Images"
    }
  }
}
